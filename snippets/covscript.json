{
	"println": {
		"prefix": "sout",
		"body": [
			"system.out.println($0)"
		],
		"description": "Print to standard output"
	},
	"getline": {
		"prefix": "getl",
		"body": [
			"system.in.getline()"
		],
		"description": "Get line from standard input"
	},
	"import": {
		"prefix": "import",
		"body": [
			"import $0"
		],
		"description": "Import a module"
	},
	"package": {
		"prefix": "package",
		"body": [
			"package $0"
		],
		"description": "Define a package"
	},
	"using": {
		"prefix": "using",
		"body": [
			"using $0"
		],
		"description": "Using alias"
	},
	"try-catch": {
		"prefix": "try",
		"body": [
			"try",
			"\t$1",
			"catch ${2:except}",
			"\t$3",
			"end"
		],
		"description": "Try-catch block"
	},
	"class/struct definition": {
		"prefix": [
			"class",
			"struct"
		],
		"body": [
			"class ${1:ClassName}",
			"\tfunction initialize()",
			"\t\t$0",
			"\tend",
			"end"
		],
		"description": "Class or struct definition"
	},
	"namespace definition": {
		"prefix": "namespace",
		"body": [
			"namespace ${1:NameSpaceName}",
			"\t$0",
			"end"
		],
		"description": "Namespace definition"
	},
	"for enumerable": {
		"prefix": "forin",
		"body": [
			"foreach ${1:value} in ${2:enumerable}",
			"\t$0",
			"end"
		],
		"description": "Foreach loop over enumerable"
	},
	"for range": {
		"prefix": "forrange",
		"body": [
			"foreach ${1:i} in range(${2:n})",
			"\t$0",
			"end"
		],
		"description": "Foreach loop over range"
	},
	"for range do": {
		"prefix": "fordo",
		"body": [
			"foreach ${1:i} in range(${2:n}) do $0"
		],
		"description": "Single-line foreach loop with range"
	},
	"if": {
		"prefix": "if",
		"body": [
			"if ${1:test}",
			"\t$0",
			"end"
		],
		"description": "If statement"
	},
	"if else": {
		"prefix": "ifelse",
		"body": [
			"if ${1:test}",
			"\t$2",
			"else",
			"\t$0",
			"end"
		],
		"description": "If-else statement"
	},
	"switch": {
		"prefix": "switch",
		"body": [
			"switch ${1:test}",
			"\t$0",
			"end"
		],
		"description": "Switch statement"
	},
	"case": {
		"prefix": "case",
		"body": [
			"case ${1:constant}",
			"\t$0",
			"end"
		],
		"description": "Case branch in switch"
	},
	"default": {
		"prefix": "default",
		"body": [
			"default",
			"\t$0",
			"end"
		],
		"description": "Default branch in switch"
	},
	"forever loop": {
		"prefix": "loop",
		"body": [
			"loop",
			"\t$0",
			"end"
		],
		"description": "Infinite loop"
	},
	"until loop": {
		"prefix": "loopuntil",
		"body": [
			"loop",
			"\t$0",
			"until ${1:test}"
		],
		"description": "Loop until condition is true"
	},
	"while loop": {
		"prefix": "while",
		"body": [
			"while ${1:test}",
			"\t$0",
			"end"
		],
		"description": "While loop"
	},
	"function definition": {
		"prefix": "function",
		"body": [
			"function ${1:functionName}(${2:params})",
			"\t$0",
			"end"
		],
		"description": "Function definition"
	}
}