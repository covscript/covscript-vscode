{
    "name": "covscript-vscode",
    "displayName": "CovScript",
    "description": "The CovScript 3 language support for VSCode",
    "version": "0.0.1",
    "publisher": "covscript",
    "repository": {
		"type": "git",
		"url": "https://github.com/covscript/covscript-vscode.git"
	},
	"bugs": {
		"url": "https://github.com/covscript/covscript-vscode/issues"
	},
	"author": {
		"name": "imkiva"
	},
	"contributors": [
    ],
    "license": "MIT",
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "snippets": [{
				"language": "covscript",
				"path": "./snippets/covscript.json"
        }],
        "languages": [{
            "id": "covscript",
            "aliases": ["CovScript", "covscript"],
            "extensions": [".csc",".csp"],
            "firstLine": "^#!\\s*/.*(?:covscript|cs)\\b",
            "filenames": [
                "csbuild.config"
            ],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "covscript",
            "scopeName": "source.covscript",
            "path": "./syntaxes/covscript.cson.json"
        }]
    }
}