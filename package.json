{
    "name": "covscript",
    "displayName": "Covariant Script",
    "description": "The CovScript 3/4 language support for VSCode",
    "version": "1.0.4",
    "publisher": "mikecovlee",
    "repository": {
        "type": "git",
        "url": "https://github.com/covscript/covscript-vscode.git"
    },
    "bugs": {
        "url": "https://github.com/covscript/covscript-vscode/issues"
    },
    "author": {
        "name": "Michael Lee"
    },
    "icon": "covscript.png",
    "contributors": [],
    "license": "MIT",
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "covscript",
                "path": "./snippets/covscript.json"
            }
        ],
        "languages": [
            {
                "id": "covscript",
                "aliases": [
                    "CovScript",
                    "covscript"
                ],
                "extensions": [
                    ".csc",
                    ".csp",
                    ".ecs"
                ],
                "firstLine": "^#!\\s*/.*(?:covscript|cs)\\b",
                "filenames": [],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "covscript",
                "scopeName": "source.covscript",
                "path": "./syntaxes/covscript.cson.json"
            }
        ]
    }
}