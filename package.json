{
    "name": "covscript",
    "displayName": "Covariant Script",
    "description": "Covariant Script 3/4 language support for VSCode",
    "version": "1.1.0",
    "publisher": "mikecovlee",
    "repository": {
        "type": "git",
        "url": "https://github.com/covscript/covscript-vscode.git"
    },
    "bugs": {
        "url": "https://github.com/covscript/covscript-vscode/issues"
    },
    "author": {
        "name": "Michael Lee"
    },
    "icon": "covscript.png",
    "contributors": [],
    "license": "MIT",
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "covscript",
        "cs",
        "language",
        "syntax highlighting"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "covscript",
                "path": "./snippets/covscript.json"
            }
        ],
        "languages": [
            {
                "id": "covscript",
                "aliases": [
                    "CovScript",
                    "covscript"
                ],
                "extensions": [
                    ".csc",
                    ".csp",
                    ".ecs",
                    ".ecsx"
                ],
                "firstLine": "^#!.*\\b(covscript|cs)\\b",
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "covscript",
                "scopeName": "source.covscript",
                "path": "./syntaxes/covscript.cson.json"
            }
        ]
    }
}